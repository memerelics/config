{
  "editor.copyWithSyntaxHighlighting": false,
  "editor.detectIndentation": false,
  "editor.fontFamily": "Ricty Diminished,Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 10,
  "editor.minimap.enabled": false,
  "editor.renderIndentGuides": false,
  "editor.suggestSelection": "first",
  "files.autoSave": "afterDelay",
  "files.eol": "\n",
  "json.schemas": [ { "fileMatch": ["*.cfn.json"], "url": "https://y13i.github.io/cfn-schema/us-west-2.min.json" } ],
  "markdown.links.openLocation": "beside",
  "markdown.preview.breaks": true,
  "markdown.preview.fontSize": 10,
  "markdown.preview.lineHeight": 1.2,
  "markdown.trace": "verbose",
  "outline.showArrays": false,
  "outline.showBooleans": false,
  "outline.showConstructors": false,
  "outline.showEnumMembers": false,
  "outline.showEvents": false,
  "outline.showFields": false,
  "outline.showFiles": false,
  "outline.showKeys": false,
  "outline.showNamespaces": false,
  "outline.showNull": false,
  "outline.showNumbers": false,
  "outline.showObjects": false,
  "outline.showOperators": false,
  "outline.showProperties": false,
  "outline.showStrings": false,
  "outline.showStructs": false,
  "outline.showTypeParameters": false,
  "outline.showVariables": false,
  "rust-analyzer.cargo.allFeatures": true,
  "rust-client.trace.server": "verbose",
  "rust.all_features": true,
  "todo-tree.tree.showScanModeButton": false,
  "todo.embedded.provider": "rg",
  "vim.autoSwitchInputMethod.switchIMCmd": "/usr/local/bin/im-select {im}",
  "vim.leader": ",",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.sideBar.location": "right",
  "yaml.customTags": [ "!And", "!Base64 mapping", "!Cidr", "!Condition", "!Equals sequence", "!FindInMap sequence", "!GetAZs", "!GetAtt scalar", "!GetAtt sequence", "!GetAtt", "!If sequence", "!ImportValue", "!Join sequence", "!Not", "!Or", "!Ref", "!Select sequence", "!Split sequence", "!Split sequence", "!Sub scalar", "!Sub scalar", "!Sub sequence", "!Sub sequence", ],
  "yaml.format.enable": true,
  "yaml.schemas": { "https://raw.githubusercontent.com/awslabs/goformation/master/schema/cloudformation.schema.json": [ "*.cfn.yaml", "*.cfn.yml" ] },

  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/target/**": true,
    "**/node_modules/*/**": true
  },
  "[json]": {
    "editor.quickSuggestions": { "strings": true },
    "editor.suggest.insertMode": "replace"
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    { "before": ["j"], "after": ["g", "j"] },
    { "before": ["k"], "after": ["g", "k"] }
  ],
  "vs-kubernetes": {
    "vs-kubernetes.draft-path": "/Users/thash/.vs-kubernetes/tools/draft/darwin-amd64/draft",
    "vs-kubernetes.helm-path": "/Users/thash/.vs-kubernetes/tools/helm/darwin-amd64/helm",
    "vs-kubernetes.kubectl-path": "/Users/thash/.vs-kubernetes/tools/kubectl/kubectl",
    "vs-kubernetes.minikube-path": "/Users/thash/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
    "vs-kubernetes.minikube-path.mac": "/Users/thash/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
  },
  "workbench.colorTheme": "Gruvbox Dark Medium",
  "rust-client.logToFile": true,
  "editor.wordWrap": "off",
  "markdown.extension.toc.orderedList": true,
  "markdown.extension.toc.omittedFromToc": {

  },
  "editor.cursorStyle": "line",
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "python.showStartPage": false,
  "workbench.tree.indent": 4,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
},
}
