	name = takuyahashimoto
	email = takuya21hashimoto@gmail.com

[color]
    status = auto
    diff = auto
    branch = auto
    interactive = auto
    grep = auto


[core]
    autocrlf = input

[alias]
    logg = log --graph --date-order --pretty=format:'%Cblue%h %Cred%an %Cgreen%ci %Cblue%m %Creset%s %Cred%d'
    st = status -s -b
    br = branch
    co = checkout
    re = remote -v
    a = add
    c = commit
    # diff
    dm = diff master
    dc = diff --cached
    d1 = diff HEAD~
    d2 = diff HEAD~~
    d3 = diff HEAD~~~
    d4 = diff HEAD~~~~
    d5 = diff HEAD~~~~~
[apply]
	whitespace = nowarn
